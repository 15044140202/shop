/* pages/set/set.wxss */
@import '/template/SetMenu/SetMenu.wxss';

.border{
  display: flex;
  flex-direction: column;
  justify-self: start;
  align-items: flex-start;
  width: 100%;
  overflow-x:hidden;
}
.logo{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin: 25rpx 25rpx 0rpx 25rpx;
  width: 100%;
  height: 150rpx;
  border-bottom: solid #bfbfbf 2rpx;
}
.logo image{
  height: 130rpx;
  width: 130rpx;
  border:#bfbfbf solid 2rpx;
  border-radius: 10%;
}
.logo_name{
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items:flex-start;
  margin-left: 30rpx;
}