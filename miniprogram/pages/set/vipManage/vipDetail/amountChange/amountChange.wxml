<!--pages/set/vipManage/vipDetail/amountChange/amountChange.wxml-->
<view wx:for="{{amountChange}}" wx:key="index" class="border">
<view style="height: 50rpx; background-color:#0097ff ; text-align: center;">序号:{{index+1}}</view>
  <view class="titel">
    <text>操作人:{{item.status}}</text>
    <text style="font-size: 28rpx;">时间:{{item.time}}</text>
  </view>
  <view class="titel">
    <text>事由:{{item.reason}}</text>
    <text>金额:{{item.changeAmount}}</text>
  </view>
</view>