<view style=" height:50rpx "></view>
<text style="margin-top:50rpx; margin-left: 50rpx; color: #999;">播报器设置</text>
<view style=" height:50rpx "></view>


<view class="titel" style="background-color: white; height: 100rpx;">
  <text style="margin-left: 30rpx;">绑定播报器</text>
  <view class="titel_1">
    <input style="width: 280rpx; margin-right: 20rpx;" type="text" value="{{device.announcer}}" placeholder="未绑定播报器" focus="false" bindblur="bindKeyInput" mark:item='announcer' />
    <van-icon name='scan' size="50rpx" color="#999" bind:tap="scan"></van-icon>
  </view>
</view>
<button type="primary" style="margin: 40rpx 40rpx 0rpx 40rpx ;" bind:tap="save">保存</button>
<button  style="margin: 40rpx 40rpx 0rpx 40rpx ;" bind:tap="announcerTest">播报测试</button>