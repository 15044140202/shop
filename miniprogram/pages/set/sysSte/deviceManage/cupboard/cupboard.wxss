/* pages/set/sysSte/deviceManage/cupboard/cupboard.wxss */
.border{
	display: flex;
	flex-direction: column;
	justify-content:center;
	align-items: center;
	width: 100%;
}
.cupboard{
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	width: 95%;
	border-radius: 20rpx;
	border: 1rpx solid #eee;
	height: 120rpx;
}
.serialNum{
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	height: 100%;
	width: 20%;
	background-color: #0097ff;
	border-top-left-radius: 20rpx;
	border-bottom-left-radius: 20rpx;
	color: #fff;
	font-size: 50rpx;
}
.cupboardInfo{
	display: flex;
	flex-direction: column;
	justify-content: space-evenly;
	align-items: flex-start;
	color: #777;
	width: 50%;
	height: 100%;
	padding-left: 20rpx;
}
.lockSet{
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: flex-start;
	height: auto;
	width: 100%;
}
.button{
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items: flex-start;
	width: 100%;
	height: auto;
}
.ruleBorder{
	display: flex;
	flex-direction: row;
	justify-content: space-evenly;
	align-items: center;
	width: 100%;
	height: 80rpx;
	border-bottom: #ccc solid 1rpx;
}
.integral{
	display: flex;
	flex-direction: row;
	justify-content: space-evenly;
	align-items: center;
	width: 45%;
	height: 100%;
}