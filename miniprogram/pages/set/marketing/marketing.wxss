.luckSudoku{
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: flex-start;
	width: 90%;
	margin-left: 5%;
	height: auto;
	margin-top: 20rpx;
	border-bottom: solid 1rpx #999;
}
.item{
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	width: 100%;
	height: 80rpx;
}
.input{
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	height: 100%;
}