.page-header{
  height: 8vh;
}
.member-status{
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  background: #fff;
  border-radius: 24rpx;
  padding: 48rpx 32rpx;
  text-align: center;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  box-shadow: 0 8rpx 24rpx rgba(0, 151, 255, 0.08);
}